{
  "expo": {
    "name": "Stent Guard Expo App",
    "description": "react native multi screen application starter with expo",
    "scheme": "expomultiscreenstarter",
    "slug": "expomultiscreenstarter",
    "privacy": "public",
    "version": "0.0.2",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/Webjayjf/stentguardexpoapp",
    "orientation": "portrait",
    "primaryColor": "#ADD8E6",
    "icon": "aa.png",
    "splash": {
      "backgroundColor": "#ADD8E6",
      "image": "aa.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["src/assets/**/*"],
    "android": {
      "versionCode": 2
    },
    "ios": {
      "buildNumber": "0.0.2",
      "supportsTablet": true
    }
  }
}
